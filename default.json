{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:js-app",
    ":automergePatch",
    ":automergeLinters",
    ":automergeTesters",
    ":automergeTypes",
    ":automergePr",
    ":ignoreUnstable",
    ":separateMultipleMajorReleases",
    ":timezone(Asia/Tokyo)",
    ":enableVulnerabilityAlerts",
    "docker:disableMajor",
    "helpers:disableTypesNodeMajor",
    "replacements:all"
  ],
  "hostRules": [
    {
      "matchHost": "https://npm.pkg.github.com/",
      "hostType": "npm",
      "encrypted": {
        "token": ""
      }
    }
  ],
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "matchCurrentVersion": "<1.0.0",
      "automerge": false
    }
  ],
  "reviewers": ["team:renovate-reviewer"]
}
